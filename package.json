{
  "name": "@trevtrich/cli",
  "description": "terminal interface to my involved tasks",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "bin": {
    "trev": "bin/trev.js"
  },
  "files": [
    "bin/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "trevtrich/cli",
  "bugs": "https://github.com/trevtrich/cli/issues",
  "homepage": "https://github.com/trevtrich/cli#readme",
  "author": "Trevor Richardson <npm@trevorrichardson.me> (trevorrichardson.me)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml",
    "clean": "rimraf ./bin",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint:peer": "npm ls >/dev/null"
  },
  "dependencies": {
    "update-notifier": "4.0.0"
  },
  "devDependencies": {
    "@babel/register": "7.8.3",
    "@travi/any": "2.0.7",
    "@travi/babel-preset": "3.0.12",
    "@travi/eslint-config": "1.0.32",
    "@travi/eslint-config-mocha": "1.0.7",
    "ban-sensitive-files": "1.9.2",
    "chai": "4.2.0",
    "codecov": "3.6.4",
    "commitlint-config-travi": "1.3.6",
    "cz-conventional-changelog": "3.1.0",
    "husky": "4.2.1",
    "mocha": "7.0.1",
    "lockfile-lint": "3.0.10",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "remark-cli": "7.0.1",
    "remark-preset-lint-travi": "1.3.0",
    "rollup": "1.31.0",
    "rimraf": "3.0.1",
    "rollup-plugin-executable": "1.5.2",
    "sinon": "8.1.0",
    "travis-lint": "1.0.0"
  }
}
